@{
    var title = SiteLibrary.AppSettings("SystemTitle");
}

<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta charset="utf-8">
<title>@title</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
<!-- Fav and touch icons -->
<link rel="shortcut icon" href="@Url.Content("~/Content/images/favicon.png")">


<!-- bootstrap & fontawesome -->
<link rel="stylesheet" href="@Url.Content("~/Content/ace/assets/css/bootstrap.min.css")" />
<link rel="stylesheet" href="@Url.Content("~/Content/ace/assets/css/font-awesome.min.css")" />


<!-- page specific plugin styles -->
<link href="@Url.Content("~/Scripts/select2/select2.css")" rel="stylesheet"><!--select2 css should first-->
<!-- text fonts -->
<link rel="stylesheet" href="@Url.Content("~/Content/ace/assets/css/ace-fonts.css")" />

<!-- ace styles -->
<link rel="stylesheet" href="@Url.Content("~/Content/ace/assets/css/ace.min.css")" id="main-ace-style" />


<!--[if lte IE 9]>
		<link rel="stylesheet" href="@Url.Content("~/Content/ace/assets/css/ace-part2.min.css")" />
<![endif]-->
<link rel="stylesheet" href="@Url.Content("~/Content/ace/assets/css/ace-skins.min.css")"/>
<link rel="stylesheet" href="@Url.Content("~/Content/ace/assets/css/ace-rtl.min.css")"/>


<!--[if lte IE 9]>
		<link rel="stylesheet" href="@Url.Content("~/Content/ace/assets/css/ace-ie.min.css")" />
<![endif]-->

<!-- ace settings handler -->
<script src="@Url.Content("~/Content/ace/assets/js/ace-extra.min.js")"></script>

<!-- HTML5shiv and Respond.js for IE8 to support HTML5 elements and media queries -->
<!--[if lte IE 8]>
  <script src="@Url.Content("~/Content/ace/assets/js/html5shiv.js")"></script>
  <script src="@Url.Content("~/Content/ace/assets/js/respond.min.js")"></script>
<![endif]-->

<link rel="stylesheet" href="@Url.Content("~/Content/docs.css")" />

<!--datepicker-->
<link rel="stylesheet" href="@Url.Content("~/Content/ace/assets/css/datepicker.css")" />

